{
  "neodev": {
    "library": {
      "enabled": true,
      "plugins": [
        "nvim-lspconfig",
        "lsp"
      ],
      "types": true,
      "runtime": true
    },
    "lspconfig": true,
    "pathStrict": true,
    "setup_jsonls": true
  },
  "neoconf": {
    "plugins": {
      "lua_ls": {
        "enabled": true
      }
    }
  },
  "lspconfig": {
    "lua_ls": {
      "Lua.completion.callSnippet": "Replace"
    }
  }
}
